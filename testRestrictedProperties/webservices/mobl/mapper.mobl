  function AMapper ( json : JSON ) {
    if ( json . errors && json . errors . length > 0 )
    {
      log(json.errors);
    }
    else
    {
      foreach ( ent : JSON in json . result ) {
        A . fromSelectJSON ( ent );
      }
    }
  }
