  import webservices :: mobl :: model
  function TopLevelEntities_Mapper ( json : JSON ) {
    foreach ( topent : JSON in json . result ) {
      if ( topent . name == "A" )
      {
        foreach ( ent : JSON in topent . value ) {
          if ( ! A . load ( ent . id ) )
          {
            A . fromSelectJSON ( ent );
          }
        }
      }
    }
  }